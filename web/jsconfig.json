{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@apis/*": [
                "src/apis/*"
            ],
            "@app-types/*": [
                "src/app-types/*"
            ],
            "@assets/*": [
                "src/assets/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@routes/*": [
                "src/routes/*"
            ],
            "@utils/*": [
                "src/utils/*"
            ],
            "@/*": [
                "src/*"
            ]
        }
    }
}